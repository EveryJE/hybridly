{
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm stub",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "pnpm lint -- --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "stub": "pnpm -r --parallel run stub",
    "test": "vitest test",
    "test:ui": "vitest test --ui --no-open",
    "test:update": "vitest -u"
  },
  "devDependencies": {
    "@innocenzi/eslint-config": "^0.7.1",
    "@sleightful/core": "workspace:*",
    "@sleightful/vite": "workspace:*",
    "@vitest/ui": "^0.5.3",
    "bumpp": "^7.1.1",
    "eslint": "^8.9.0",
    "happy-dom": "^2.41.0",
    "sleightful": "workspace:*",
    "type-fest": "^2.12.0",
    "typescript": "^4.5.5",
    "unbuild": "^0.6.9",
    "vite": "^2.8.4",
    "vitest": "^0.5.3"
  }
}
